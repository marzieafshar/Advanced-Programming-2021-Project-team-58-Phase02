<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="650.0" prefWidth="1214.0" styleClass="Register-background" stylesheets="@../Styles.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Client.View.Controller">
    <children>
        <Button layoutX="12.0" layoutY="604.0" mnemonicParsing="false" onAction="#backToWelcomeMenu" prefHeight="33.0" prefWidth="81.0" styleClass="profile-backButton" text="Back">
         <font>
            <Font name="System Bold" size="16.0" />
         </font></Button>
        <Button layoutX="203.0" layoutY="507.0" mnemonicParsing="false" onAction="#Register" prefHeight="47.0" prefWidth="94.0" styleClass="register-button" stylesheets="@../Styles.css" text="Register">
         <font>
            <Font size="18.0" />
         </font></Button>
        <TextField fx:id="usernameRegisterField" layoutX="109.0" layoutY="206.0" prefHeight="60.0" prefWidth="281.0" promptText="Username" styleClass="register-button">
            <font>
                <Font name="System Bold" size="24.0" />
            </font>
        </TextField>
        <TextField fx:id="nicknameRegisterField" layoutX="110.0" layoutY="291.0" prefHeight="60.0" prefWidth="281.0" promptText="Nickname" styleClass="register-button">
            <font>
                <Font name="System Bold" size="24.0" />
            </font>
        </TextField>
        <TextField fx:id="passwordRegisterField" layoutX="113.0" layoutY="378.0" prefHeight="60.0" prefWidth="281.0" promptText="Password" styleClass="register-button">
            <font>
                <Font name="System Bold" size="24.0" />
            </font>
        </TextField>
      <Label layoutX="56.0" layoutY="48.0" prefHeight="96.0" prefWidth="387.0" text="Register Menu" textFill="WHITE">
         <font>
            <Font name="Matura MT Script Capitals" size="53.0" />
         </font>
      </Label>
    </children>
</AnchorPane>
