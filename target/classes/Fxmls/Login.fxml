<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="login-background" stylesheets="@../Styles.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.Controller">
    <children>
        <TextField fx:id="usernameLoginField" layoutX="66.0" layoutY="145.0" prefHeight="52.0" prefWidth="224.0" promptText="Username" styleClass="login-field">
            <font>
                <Font size="18.0" />
            </font>
        </TextField>
        <TextField fx:id="passwordLoginField" layoutX="66.0" layoutY="217.0" prefHeight="52.0" prefWidth="224.0" promptText="Password" styleClass="login-field">
            <font>
                <Font size="18.0" />
            </font>
        </TextField>
        <Button layoutX="151.0" layoutY="297.0" mnemonicParsing="false" onAction="#Login" styleClass="login-button" text="Login" />
        <Button layoutX="14.0" layoutY="355.0" mnemonicParsing="false" onAction="#backToWelcomeMenu" prefHeight="31.0" prefWidth="65.0" styleClass="back-button" text="back" textFill="WHITE" />
      <Label layoutX="85.0" layoutY="74.0" prefHeight="53.0" prefWidth="180.0" text="Login Menu" textAlignment="CENTER">
         <font>
            <Font name="Berlin Sans FB Demi Bold" size="34.0" />
         </font>
      </Label>
    </children>
</AnchorPane>
